{
	"extends": "tslint:recommended",
	"rules": {
		"indent": [
			true,
			"tabs"
		],
		"no-console": false,
		"no-unused-variable": true,
		"ordered-imports": [
			true,
			{
				"grouped-imports": true
			}
		],
		"object-literal-sort-keys": false,
		"interface-name": false,
		"max-line-length": false,
		"interface-over-type-literal": false
	}
}
